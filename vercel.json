{
    "version": 2,
    "routes": [
      { "src": "/(.*)", "dest": "/api/index.js" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
    ],
    "functions": {
      "api/index.js": {
        "runtime": "nodejs22.x"
      }
    }
}